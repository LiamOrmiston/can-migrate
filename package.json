{
  "name": "can-migrate-codemods",
  "version": "0.0.1",
  "description": "Codemods to simplify upgrading CanJS version",
  "main": "lib/",
  "scripts": {
    "generate": "./build/generate.js",
    "compile": "rm -rf lib/ && babel -d lib/ src/",
    "watch": "babel --watch -d lib/ src/",
    "jshint": "jshint src/. test/. --config",
    "mocha": "mocha test/ --compilers js:babel-core/register",
    "test": "npm run generate && npm run compile && npm run jshint && npm run mocha"
  },
  "bin": {
    "can-migrate": "./bin/can-migrate.js"
  },
  "directories": {
    "lib": "lib"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "preferGlobal": true,
  "author": {
    "name": "Bitovi",
    "email": "curtis@pixelplicity.com"
  },
  "license": "MIT",
  "dependencies": {
    "disparity": "^2.0.0",
    "execa": "^0.5.0",
    "globby": "^6.0.0",
    "js-codemod": "^7.0.0",
    "jscodeshift": "^0.3.29",
    "jscodeshift-imports": "^1.0.1",
    "jsesc": "^2.2.0",
    "meow": "^3.7.0",
    "promise-map-series": "^0.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.16.0",
    "ejs": "^2.5.2",
    "fs-extra": "^0.30.0",
    "js-beautify": "^1.6.4",
    "jshint": "^2.9.3",
    "mocha": "^3.1.2"
  }
}
